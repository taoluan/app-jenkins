pipeline {
  agent any
  stages {
    stage('SCM') {
      steps {
        checkout scm
      }
    }

    stage('SonarQube Analysis') {
      steps {
        withSonarQubeEnv('Scan') {
            sh "sonar-scanner -Dsonar.projectKey=jenkins -Dsonar.sources='jenkins'"
        }
      }
    }
  }
}
